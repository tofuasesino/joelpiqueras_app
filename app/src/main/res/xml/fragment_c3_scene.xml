               android:rotation="-20"
               motion:motionTarget="@+id/nokia"
               motion:framePosition="80"
               android:scaleX="1.5"
               android:scaleY="1.5"
               />
       </KeyFrameSet>
        <OnClick/>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            />
    </ConstraintSet>
</MotionScene>