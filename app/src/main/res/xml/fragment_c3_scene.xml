               android:rotation="-20"
               motion:motionTarget="@+id/nokia"
               motion:framePosition="10"
               android:scaleX="1.5"
               android:scaleY="1.5"
               />
           <KeyAttribute
               android:rotation="20"
               motion:motionTarget="@+id/nokia"
               motion:framePosition="15"
               android:scaleX="1.5"
               android:scaleY="1.5"
               />
           <KeyAttribute
               android:rotation="-20"
               motion:motionTarget="@+id/nokia"
               motion:framePosition="20"
               android:scaleX="1.5"
               android:scaleY="1.5"
               />
           <KeyAttribute
               android:rotation="20"
               motion:motionTarget="@+id/nokia"
               motion:framePosition="25"
               android:scaleX="1.5"
               android:scaleY="1.5"
               />
           <KeyAttribute
               android:rotation="-20"
               motion:motionTarget="@+id/nokia"
               motion:framePosition="30"
               android:scaleX="1.5"
               android:scaleY="1.5"
               />
           <KeyAttribute
               android:rotation="20"
               motion:motionTarget="@+id/nokia"
               motion:framePosition="55"
               android:scaleX="1.5"
               android:scaleY="1.5"
               />
           <KeyAttribute
               android:rotation="-20"
               motion:motionTarget="@+id/nokia"
               motion:framePosition="60"
               android:scaleX="1.5"
               android:scaleY="1.5"
               />
           <KeyAttribute
               android:rotation="20"
               motion:motionTarget="@+id/nokia"
               motion:framePosition="65"
               android:scaleX="1.5"
               android:scaleY="1.5"
               />
           <KeyAttribute
               android:rotation="-20"
               motion:motionTarget="@+id/nokia"
               motion:framePosition="70"
               android:scaleX="1.5"
               android:scaleY="1.5"
               />
           <KeyAttribute
               android:rotation="20"
               motion:motionTarget="@+id/nokia"
               motion:framePosition="75"
               android:scaleX="1.5"
               android:scaleY="1.5"
               />
